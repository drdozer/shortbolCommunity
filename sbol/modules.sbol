
@prefix sbol <http://sbols.org/v2#>
@prefix i <http://sbols.org/v2/interaction>
@prefix p <http://sbols.org/v2/participation>

# Interaction type values

Phosphorylation     = <http://identifiers.org/biomodels.sbo/SBO:0000216>
Inhibition          = <http://identifiers.org/biomodels.sbo/SBO:0000169>
Stimulation         = <http://identifiers.org/biomodels.sbo/SBO:0000170>
BiochemicalReaction = <http://identifiers.org/biomodels.sbo/SBO:0000176>
NonCovalentBinding  = <http://identifiers.org/biomodels.sbo/SBO:0000177>
Degradation         = <http://identifiers.org/biomodels.sbo/SBO:0000179>
GeneticProduction   = <http://identifiers.org/biomodels.sbo/SBO:0000589>

# Participation role values
Inhibitor  = <http://identifiers.org/biomodels.sbo/SBO:0000020>
Stimulator = <http://identifiers.org/biomodels.sbo/SBO:0000459>
Reactant   = <http://identifiers.org/biomodels.sbo/SBO:0000010>
Product    = <http://identifiers.org/biomodels.sbo/SBO:0000011>
Promoter   = <http://identifiers.org/biomodels.sbo/SBO:0000598>
Modifier   = <http://identifiers.org/biomodels.sbo/SBO:0000019>
Acceptor   = <http://identifiers.org/psimi/MI:0843>
Donor      = <http://identifiers.org/psimi/MI:0842>
Template   = <http://identifiers.org/SBO:0000645>


PairInteraction(sbj, obj, tpe, sbjRole, objRole) => sbol:Interaction
  sbol:type = tpe
  sbol:participation : sbol:Participation
    sbol:participant = sbj
    sbol:role = sbjRole
  sbol:participation : sbol:Participation
    sbol:participant = obj
    sbol:role = objRole

phosphorylates(sbj, obj) => PairInteraction(sbj, obj, Phosphorylation, Donor, Acceptor)

encodes(sbj, obj) => PairInteraction(sbj, obj, GeneticProduction, Template, Product)

represses(sbj, obj) => PairInteraction(sbj, obj, Inhibition, Inhibitor, Inhibited)